<h1>Create Links</h1>
<%= form_for @link do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.submit %>
<% end %>

<h1>All Links</h1>

<% @links.each do |link| %>
<ul>
  <li><%= link.title %></li>
  <li><%= link.url %></li>
  <li><%= link.read %></li>
</ul>
<% end %>
